<h2>Users list</h2>
<a href="/">Back to main page</a>
<div class="search-box">
    <form action="/users/search" method="GET" class="search-form" id="search-form">
        <input type="search" placeholder="Search by name" name="q" id="name-input">
        <select class="search-select" name="start" id="start-select">
            <option selected hidden>Select age from</option>
            {{#each ageRange}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
        <select class="search-select" name="end" id="end-select">
            <option selected hidden>Select age to</option>
            {{#each ageRange}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
    </form>
</div>
<div id="pagination-area">
{{> userstable }}
</div>
<script src="/javascripts/userSearcher.js"></script>